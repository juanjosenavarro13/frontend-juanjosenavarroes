    "husky": "9.0.11",
    "i18next": "23.11.5",
    "i18next-http-backend": "2.5.2",
    "install": "0.13.0",
    "lint-staged": "15.2.7",
    "path": "^0.12.7",
    "postcss": "8.4.39",
    "prettier": "3.3.3",
    "prettier-plugin-tailwindcss": "0.6.4",
    "react-hook-form": "7.52.1",
    "react-i18next": "14.1.2",
    "react-router-dom": "6.24.0",
    "tailwindcss": "3.4.4",
    "typescript": "5.5.3",
    "vite": "5.3.2",
    "vitest": "1.6.0",
    "zod": "3.23.8",
