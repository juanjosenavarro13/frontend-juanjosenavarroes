{
  "name": "juanjosenavarroes",
  "author": "Juan Jos√© Navarro Perea",
  "private": true,
  "version": "0.4.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "lint": "eslint --ext ts,tsx,js . --fix",
    "lint:check": "eslint . --ext ts,tsx,js --report-unused-disable-directives --max-warnings 0",
    "format": "prettier --write src",
    "format:check": "prettier --check src",
    "test": "vitest",
    "test:ui": "vitest --coverage --ui",
    "test:coverage": "vitest --coverage --no-watch",
    "delete-old-branch": "git remote prune origin && git branch -vv | grep ': gone]' | awk '{print $1}' | xargs -r git branch -d",
    "version:patch": "npm version patch && git push && git push --tags",
    "version:minor": "npm version minor && git push && git push --tags",
    "version:major": "npm version major && git push && git push --tags",
    "prepare": "husky"
  },
  "dependencies": {
    "react": "18.3.1",
    "react-dom": "18.2.0"
  },
  "devDependencies": {
    "@commitlint/cli": "19.3.0",
    "@commitlint/config-conventional": "19.2.2",
    "@hookform/resolvers": "3.3.4",
    "@tanstack/react-query": "5.31.0",
    "@tanstack/react-query-devtools": "5.28.12",
    "@testing-library/jest-dom": "6.4.2",
    "@testing-library/react": "15.0.5",
    "@types/node": "20.12.11",
    "@types/react": "18.3.1",
    "@types/react-dom": "18.2.23",
    "@typescript-eslint/eslint-plugin": "7.7.1",
    "@typescript-eslint/parser": "7.8.0",
    "@vercel/analytics": "1.2.2",
    "@vercel/speed-insights": "1.0.10",
    "@vitejs/plugin-react-swc": "3.6.0",
    "@vitest/coverage-v8": "1.6.0",
    "@vitest/ui": "1.6.0",
    "autoprefixer": "10.4.19",
    "axios": "1.6.8",
    "eslint": "8.57.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-react-hooks": "4.6.2",
    "eslint-plugin-react-refresh": "0.4.6",
    "happy-dom": "14.7.1",
    "husky": "9.0.11",
    "i18next": "23.11.4",
    "i18next-http-backend": "2.5.0",
    "install": "0.13.0",
    "lint-staged": "15.2.2",
    "postcss": "8.4.38",
    "prettier": "3.2.5",
    "prettier-plugin-tailwindcss": "0.5.14",
    "react-hook-form": "7.51.2",
    "react-i18next": "14.1.0",
    "react-router-dom": "6.22.3",
    "tailwindcss": "3.4.3",
    "typescript": "5.4.5",
    "vite": "5.2.11",
    "vitest": "1.6.0",
    "zod": "3.22.4",
    "zustand": "4.5.2"
  }
}
